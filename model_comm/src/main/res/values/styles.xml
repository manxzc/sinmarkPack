<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppTheme_App" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>

        <item name="android:activityOpenEnterAnimation">@anim/anim_alpha_enter</item>
        <item name="android:activityOpenExitAnimation">@anim/anim_alpha_exit</item>
        <item name="android:activityCloseEnterAnimation">@anim/anim_alpha_enter</item>
        <item name="android:activityCloseExitAnimation">@anim/anim_alpha_exit</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

